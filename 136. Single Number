class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        #return [i for i in nums if nums.count(i)!=2].pop() #exceed time limit
        return (2*sum(set(nums))-sum(nums))
        
